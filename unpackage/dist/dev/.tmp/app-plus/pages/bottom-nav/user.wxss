
.Real_name {
	height: 46rpx;
	border: #687c99 solid 3rpx;
	margin-left: 250rpx;
	border-radius: 25rpx;
	line-height: 46rpx;
	color: #687c99;
	padding: 0 15rpx;
	font-size: 28rpx;
}
.active {
	border: #7ce2f5 solid 3rpx;
	color: #7ce2f5;
}
.titlenav {
	width: 94%;
	background: #091629;
	border-radius: 30rpx;
	margin: 0 auto;
}
.float {
	width: 40%;
	margin: 13rpx;
	padding: 20rpx;
	position: relative;
}
.textnav {
	color: #687c99;
	font-size: 30rpx;
}
.textnav3 {
	color: #687c99;
	font-size: 28rpx;
	margin-top: 80rpx;
	text-align: center;
}
.username {
	color: white;
	font-size: 28rpx;
}
.listnav {
	margin: 0 auto;
	width: 88%;
	height: 40rpx;
	border-top: #1f2a3e solid 0.5rpx;
	line-height: 40rpx;
	border-left: none;
	border-right: none;
	padding: 20rpx;
}
.icon {
	width: 65rpx;
	height: 60rpx;
	position: absolute;
	left: 50%;
	margin-left: -32rpx;
}
.login-btn {
	padding: 10rpx 20rpx;
	margin-top: 50rpx;
}
.landing {
	width: 50%;
	height: 84rpx;
	line-height: 84rpx;
	border-radius: 10rpx;
	font-size: 32rpx;
	background: -webkit-gradient(linear, left top, right top, from(#4286de), to(#5091e6));
	background: -o-linear-gradient(left, #4286de, #5091e6);
	background: linear-gradient(left, #4286de, #5091e6);
}

