<view style="padding:40rpx;"><view class="titlebox"><view class="typetext">钱包地址：</view><input class="inputext" placeholder="{{codeurl==null?'请输入钱包地址':codeurl}}"/></view><view class="titlebox"><view class="typetext">可用金额：</view><input class="inputext" disabled placeholder="{{message.num-message.dongjie}}"/></view><view class="titlebox"><view class="typetext">冻结金额：</view><input class="inputext" disabled placeholder="{{message.dongjie}}"/></view><view class="titlebox"><view class="typetext">数量：</view><input class="inputext" type="number" placeholder="请输入转出数量" data-event-opts="{{[['input',[['__set_model',['','num','$event',[]]]]]]}}" value="{{num}}" bindinput="__e"/></view><view class="titlebox"><view class="typetext">钱包密码：</view><input class="inputext" type="password" placeholder="请输入钱包密码" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" bindinput="__e"/></view><button class="landing" type="primary" data-event-opts="{{[['tap',[['get_acounts',['$event']]]]]}}" bindtap="__e">确认</button></view>