<view><view class="forget-bg"><view class="forget-card"><view class="forget-input"><input placeholder="请输入昵称" data-event-opts="{{[['input',[['__set_model',['','name','$event',[]]]]]]}}" value="{{name}}" bindinput="__e"/></view><view class="forget-input"><input password="true" placeholder="请输入密码(8-18位)" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" value="{{password}}" bindinput="__e"/></view><view class="forget-input"><input type="password" placeholder="请输入确认密码(8-18位)" data-event-opts="{{[['input',[['__set_model',['','confirm','$event',[]]]]]]}}" value="{{confirm}}" bindinput="__e"/></view><view class="forget-input"><input type="password" placeholder="请设置钱包密码(8-18位)" data-event-opts="{{[['input',[['__set_model',['','qianbao','$event',[]]]]]]}}" value="{{qianbao}}" bindinput="__e"/></view><view class="forget-input forget-margin-b"><view class="verify-left"><input type="number" placeholder="请输入验证码" data-event-opts="{{[['input',[['__set_model',['','code','$event',[]]]]]]}}" value="{{code}}" bindinput="__e"/></view><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="verify-right" bindtap="__e"><button class="verify-btn" type="primary">{{!safety.state&&'获取验证码'||safety.time+' s'}}</button></view></view><view class="forget-input"><input class="noChinese" placeholder="请输入推荐码" data-event-opts="{{[['input',[['__set_model',['','p','$event',[]]]]]]}}" value="{{p}}" bindinput="__e"/></view></view></view><view data-event-opts="{{[['tap',[['getRegiter',['$event']]]]]}}" class="forget-btn" bindtap="__e"><button class="landing" type="primary">注册</button></view></view>